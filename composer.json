{
    "name": "getpop/bootstrap-wp",
    "type": "project",
    "description": "Bootstrap a PoP project for WordPress",
    "keywords": [
        "PoP",
        "Bootstrap",
        "WordPress"
    ],
    "homepage": "https://github.com/getpop/bootstrap-wp",
    "license": "MIT",
    "authors": [
        {
            "name": "Leonardo Losoviz",
            "email": "leo@getpop.org",
            "homepage": "https://getpop.org",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {
        "johnpbloch/wordpress": ">=5.1"
    },
    "extra": {
        "wordpress-install-dir": "wp",
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "vendor-dir": "wp-content/vendor",
        "sort-packages": true
    }
}